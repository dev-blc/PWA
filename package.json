            "ignoreMissing": [
                "webpack",
                "typescript"
            ],
            "allowAny": [
                "typescript",
                "webpack"
            ]
        },
        "onlyBuiltDependencies": [
            "bigint-buffer",
            "bufferutil",
            "core-js-pure",
            "esbuild",
            "fsevents",
            "keccak",
            "sharp",
            "utf-8-validate"
        ]
