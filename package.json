    "prettier": {
        "arrowParens": "avoid",
        "bracketSameLine": true,
        "bracketSpacing": true,
        "endOfLine": "lf",
        "jsxSingleQuote": true,
        "printWidth": 120,
        "proseWrap": "preserve",
        "quoteProps": "consistent",
        "semi": false,
        "singleQuote": false,
        "trailingComma": "all",
        "useTabs": false,
        "tabWidth": 4,
        "plugins": [
            "prettier-plugin-tailwindcss"
        ],
        "tailwindConfig": "./tailwind.config.ts",
        "tailwindStylesheet": "./src/app/globals.css",
        "tailwindFunctions": [
